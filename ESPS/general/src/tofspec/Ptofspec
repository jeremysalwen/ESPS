#  @(#)Ptofspec	1.3	1/25/93	ESI 
# default parameter file for tofspec
string	field_name ?= "sd": "Input field name";
int	start ?= 1: "First record from input file to process";
int	nan ?= 0: "Number of records from input file to process[0 means all]";
string	low_input ?= "determine from file": "clip below this input level";
string 	high_input ?= "determine from file": "clip above this input level";
float	low_output ?= -64: "lower limit of output range";
float   high_output ?= 50: "upper limit of output range";
float   sf ?= 0: "original sampling rate (0 means set to 2*(field_size - 1))";
