text
@#! /bin/sh
echo Compiled on `uname -a` >$ESPS_BASE/bin/compiled
date >>$ESPS_BASE/bin/compiled
set -x
ua=`uname -a`
if echo $ua | grep -q -s "[Ll]inux" ; then export ARCH=linux
elif echo $ua | egrep -q -s "[Dd]arwin|[Mm]ac" ; then export ARCH=mac
fi
current_dir=`pwd`
cd $current_dir/../h; emake clean; emake install; cd $current_dir
if test x$1 = xlibs
 then
  cd ../../ATT/sigproc; emake clean install; cd $current_dir
  cd ../../ATT/libsig; emake clean install; cd $current_dir
  cd lib; emake clean; emake install; cd $current_dir
  cd lib_header/sphere2.6; make clean depend install PROJDIR=`pwd`; \
	cd $current_dir
  cd lib_header; emake clean; emake install; cd $current_dir
  cd lib_sp; emake clean; emake install; cd $current_dir
  cd libxv; emake clean; emake install; cd $current_dir
fi
cd utils; emake clean; emake install; cd $current_dir
cd $current_dir/../man;  emake install
cd $current_dir

AUDIO=none
case `mach_type` in
	SUN4) 	AUDIO=s16audio ;;
	HP700) 	AUDIO=hpaudio ;;
	SG)	AUDIO=sgaudio ;;
esac
# HERE IS WHERE XWAVES, ETC. GET MADE....
cd ../../ATT ; emake clean install;  cd $current_dir
cd ${ARCH}_audio; emake clean; emake install; cd $current_dir
cd acf; emake clean; emake install; cd $current_dir
cd addclass; emake clean; emake install; cd $current_dir
cd addfea; emake clean; emake install; cd $current_dir
cd addfeahd; emake clean; emake install; cd $current_dir
cd addgen; emake clean; emake install; cd $current_dir
