"Verbosity" MENU
	"0" set verbose 0
	"1" set verbose 1
	"2" set verbose 2
"Verbosity" END
"Debug Level" MENU
	"0" set debug_level 0
	"1" set debug_level 1
	"2" set debug_level 2
	"3" set debug_level 3
"Debug Level" END
"Single Step" MENU
	"ON " set command_step 1
	"OFF" set command_step 0
"Single Step" END

